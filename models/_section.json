{
  "definitions": [
    {
      "title": "Section",
      "id": "section",
      "plugins": {
        "xwalk": {
          "page": {
            "resourceType": "core/franklin/components/section/v1/section",
            "template": {
              "model": "section"
            }
          }
        }
      }
    }
  ],
  "models": [
    {
      "id": "section",
      "fields": [
        {
          "component": "text",
          "name": "name",
          "label": "Section Name",
          "description": "The label shown for this section in the Content Tree"
        },
        {
          "component": "text",
          "name": "id",
          "label": "Section ID",
          "description": "Unique identifier for this section (HTML ID)",
          "validation": {
            "regex": "^[a-zA-Z][\\w:.-]*$"
          }
        },
        {
          "component": "multiselect",
          "name": "generic-style",
          "label": "Generic Styles",
          "options": [
            { "name": "Top Spacing", "value": "top-margin" },
            { "name": "Bottom Spacing", "value": "bottom-margin" },
            { "name": "Top Inner Spacing", "value": "top-padding" },
            { "name": "Bottom Inner Spacing", "value": "bottom-padding" },
            { "name": "Bottom Partition", "value": "bottom-partition" },
            { "name": "Top Spacer", "value": "spacer-top" },
